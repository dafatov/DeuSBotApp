{
  "name": "ru.demetrious.discord-bot.deus",
  "version": "1.14.1",
  "description": "A bot for discord by DemetriouS",
  "main": "src/main/js/bot.js",
  "type": "commonjs",
  "private": true,
  "config": {
    "isPublic": true,
    "semver": "prerelease"
  },
  "scripts": {
    "start": "node -r dotenv/config src/main/js/bot.js",
    "lint": "eslint ./src",
    "i18Scanner": "i18next-scanner --config src/main/js/configs/i18nextScanner.js",
    "prepare": "husky install",
    "test": "jest",
    "bump": "grunt bump-only:%npm_package_config_semver%",
    "postbump": "npm pkg set config.semver=prerelease && npm install && git add package.json package-lock.json"
  },
  "author": "DemetriouS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dafatov/DeusBot.git"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.16.0",
    "@octokit/core": "4.2.4",
    "axios": "1.6.3",
    "body-parser": "1.20.2",
    "canvas": "2.11.2",
    "cors": "2.8.5",
    "discord.js": "14.11.0",
    "express": "4.18.2",
    "ffmpeg-static": "5.1.0",
    "get-audio-duration": "4.0.1",
    "i18next": "23.3.0",
    "i18next-fs-backend": "2.1.5",
    "is-url": "1.2.4",
    "is-youtube-url": "1.0.5",
    "lodash": "4.17.21",
    "pg": "8.11.1",
    "pg-format": "1.0.4",
    "postgres-interval": "4.0.1",
    "postgres-migrations": "5.3.0",
    "random-org": "2.2.0",
    "rss-parser": "3.13.0",
    "socket.io": "4.7.1",
    "sodium-native": "4.0.4",
    "string-progressbar": "1.0.4",
    "xml2js": "0.6.2",
    "ytdl-core": "4.11.5",
    "ytpl": "2.3.0",
    "ytsr": "3.8.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-arguments": "^1.0.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-sort-imports-requires": "^1.0.2",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.4.3",
    "husky": "^8.0.3",
    "i18next-scanner": "^4.3.0",
    "jest": "^29.6.2",
    "socket.io-client": "4.7.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "16.13.0"
  }
}
